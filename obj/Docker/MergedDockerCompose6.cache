services:
  menuql:
    build:
      context: E:\belajar\bootcamp\final\FoodDeliveryApp\FoodDeliveryApp
      dockerfile: MenuQL/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: MenuQL
      target: base
    container_name: MenuQL
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: menuql:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net6.0/MenuQL.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - E:\belajar\bootcamp\final\FoodDeliveryApp\FoodDeliveryApp\MenuQL:/app:rw
    - C:\Users\ahmad saputra\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\ahmad saputra\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\ahmad saputra\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\ahmad saputra\.nuget\packages:/root/.nuget/packages:ro
    - E:\belajar\bootcamp\final\FoodDeliveryApp\FoodDeliveryApp:/src:rw
  orderql:
    build:
      context: E:\belajar\bootcamp\final\FoodDeliveryApp\FoodDeliveryApp
      dockerfile: OrderQL/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: orderql
    ports:
    - target: 80
    - target: 443
    volumes:
    - C:\Users\ahmad saputra\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
  userql:
    build:
      context: E:\belajar\bootcamp\final\FoodDeliveryApp\FoodDeliveryApp
      dockerfile: UserQL/Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: userql
    ports:
    - target: 80
    - target: 443
    volumes:
    - C:\Users\ahmad saputra\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
version: '3.4'